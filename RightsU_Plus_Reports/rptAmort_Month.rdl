  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DS_Reports</DataSourceName>
        <CommandText>SELECT        AD.Catchup_Amount, ADC.Deal_Cost, TC.Episode_Title, A.Agreement_No, 
                         SUBSTRING('C_JAN D_FEB E_MAR F_APR G_MAY H_JUN I_JUL J_AUG K_SEP L_OCT A_NOV B_DEC ', AD.Month * 6 - 5, 5) 
                         + '''' + SUBSTRING(CAST(AD.Year AS varchar(20)), 3, LEN(CAST(AD.Year AS varchar(20))) - 2) AS Month_Year, AD.Amorted_Amount
FROM            Amorted_Data AS AD INNER JOIN
                         Amort_Month AS AM ON AD.Amort_Month_Code = AM.Amort_Month_Code INNER JOIN
                         Title_Content AS TC ON TC.Title_Content_Code = AD.Title_Content_Code INNER JOIN
                         Title_Content_Mapping AS TCM ON TCM.Title_Content_Code = TC.Title_Content_Code INNER JOIN
                         Acq_Deal_Movie AS ADM ON ADM.Acq_Deal_Movie_Code = TCM.Acq_Deal_Movie_Code INNER JOIN
                         Acq_Deal AS A ON A.Acq_Deal_Code = ADM.Acq_Deal_Code INNER JOIN
                         Acq_Deal_Cost AS ADC ON ADC.Acq_Deal_Code = A.Acq_Deal_Code</CommandText>
      </Query>
      <Fields>
        <Field Name="Catchup_Amount">
          <DataField>Catchup_Amount</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
