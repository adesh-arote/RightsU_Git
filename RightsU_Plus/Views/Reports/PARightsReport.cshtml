    function txtPageSize_OnChange() {
        $("[required='required']").removeAttr("required");
        $('.required').removeClass('required');
        if (!ValidatePageSize())
            return false;

        BindDealDataList()
        //SetPaging();
    }
    function SetPaging() {
        debugger
        IsCall = 'N';
        var pageNo = parseInt($('#hdnPageNo').val());
        var recordCount = parseInt($('#hdnRecordCount').val());
        var pagePerBatch = parseInt($('#hdnPagePerBatch').val());
        var recordPerPage = parseInt($('#txtPageSize').val());

        var cnt = pageNo * recordPerPage;
        if (cnt >= recordCount) {
            var v1 = parseInt(recordCount / recordPerPage);
            if ((v1 * recordPerPage) == recordCount)
                pageNo = v1;
            else
                pageNo = v1 + 1;
        }

        if (pageNo == 0)
            pageNo = 1;

        var index = pageNo - 1;
        $('#hdnPageNo').val(pageNo);

        var opt = null;
        opt = { callback: pageselectCallback };
        opt["items_per_page"] = recordPerPage;
        opt["num_display_entries"] = pagePerBatch;
        opt["num"] = 10;
        opt["prev_text"] = "<<";
        opt["next_text"] = ">>";
        opt["current_page"] = index;
        $("#Pagination").pagination(recordCount, opt);
    }
    function pageselectCallback(page_index, jq) {
        $('.required').removeClass('required');

        if (!ValidatePageSize())
            return false;

        var pageNo = page_index + 1
        $('#hdnPageNo').val(pageNo);
        if (IsCall == 'Y')
            BindDealDataList();
        else
            IsCall = 'Y';
    }
    function ValidatePageSize() {
        var recordPerPage = $('#txtPageSize').val()
        if ($.trim(recordPerPage) != '') {
            var pageSize = parseInt(recordPerPage);
            if (pageSize > 0)
                return true;
        }
        $('#txtPageSize').attr('required', true)
        return false
    }
    function btnShow_OnClick() {
        debugger;
        if (validateSave('CHK')) {
            $('#txtCriteriaName').val('');
            $('input:radio[name=AccessibilityType][id=rdoPub]').prop('checked', true);
            //$('#popup1').modal();
            $('#popup').modal();
        }
    }

    function validateSave(callFrom) {
        debugger;
        $('.required').removeClass('required');
        $("[required='required']").removeAttr("required");
        var returnVal = true;
        var str = "";
        var agreementNo = $('#txtAgreementNo').val();
        var businessUnitcode = $('#ddlBusinessUnit').val();
        var Content = $('#txtTitle').val();
        

        // if ($('#txtTitle').val() == 0 && $('#txtfrom').val() == '' && $('#txtto').val() == '' && $('#ddlChannelList').val() == null && $('#ddlTitleTypeList').val() == null && $('#ddlMusicLabelList').val() == null && $('#txtStar').val() == 0 && $('#ddlGenresList').val() == null && $('#ddlMusicThemeList').val() == null) {
        if ($('#txtAgreementNo').val() == 0 && $('#ddlBusinessUnit').val() == '' && $('#txtTitle').val() == '') {
            showAlert('E', 'Please Select at least one search criteria');
            return false;
        }
        var IsAncillary = $('#hdnIsAncillary').val();
        var platformCodes = "";
        var AncillaryTypeCode = {};
        if (IsAncillary == "Y") {

            if ($('#hdnTitleCode').val() == '0' && ($('#ddlAncillaryType').val() == "" || $('#ddlAncillaryType').val() == null)) {
                showAlert('E', '@objMessageKey.PleaseSelectatleastTitleOrAncillaryType');
                return false;
            }

            platformCodes = platformCodes_G;
            if ($('#ddlAncillaryType').val() != "" && $('#ddlAncillaryType').val() != 0 && $('#ddlAncillaryType').val() != null) {
                AncillaryTypeCode = $('#ddlAncillaryType').val().join(',');//$('#ddlAncillaryType').val();
            }
            else
                AncillaryTypeCode = "";

        }
        else {
            if ($('#hdnTitleCode').val() == '0' && ($('#txtAgreementNo').val() == null || agreementNo == "")) {
                showAlert('E', '@objMessageKey.PleaseSelectatleastonesearchcriteria');
                return false;
            }
        }
        var IncludeExpired = "";
        if ($('#IncludeExpired').is(':checked')) {
            IncludeExpired = "Y";
        }
        else
            var IncludeExpired = "N";
        
        if (callFrom == 'FS') {

            var txtCriteriaName = $.trim($('#txtCriteriaName').val());
            var AccessibilityType = $("input[name='AccessibilityType']:checked").val(); // PR OR PU

            if (txtCriteriaName == "") {
                $('#txtCriteriaName').val('')
                $('#txtCriteriaName').attr('required', true)
                returnVal = false;
                return false
            }
            if (returnVal) {
                $.ajax({
                    type: "POST",
                    url: URL_BindPARightsReport,
                    traditional: true,
                    enctype: 'multipart/form-data',
                    contentType: "application/json; charset=utf-8",
                    data: JSON.stringify({
                        txtCriteriaName: txtCriteriaName,
                        AccessibilityType: AccessibilityType,
                        agreementNo: agreementNo,
                        businessUnitcode: businessUnitcode,
                        titleCodes: Content,
                        AncillaryTypeCode: AncillaryTypeCode,
                        platformCodes: platformCodes,
                        IncludeExpired : IncludeExpired,
                    }),
                    async: false,
                    success: function (result) {
                        if (result == "true") {
                            redirectToLogin();
                        }
                        else {
                            if (result.Status == 'S') {
                                showAlert('S', result.Message);
                                ClosePopup();
                                BindDealDataList();
                                ClearAll();
                            }
                            else
                                showAlert('E', result.Message);
                        }
                    },
                    error: function (result) {
                        alert('Error: ' + result.responseText);
                    }
                });
            }
        }

        return returnVal;
    }

    function BindDealDataList() {
        debugger;
        var pageNo = $('#hdnPageNo').val();
        var recordPerPage = $('#txtPageSize').val();
        var BUCode = $("#ddlBusinessUnit").val();
        $.ajax({
            type: "POST",
            url: URL_BindPARList,
            traditional: true,
            enctype: 'multipart/form-data',
            contentType: "application/json; charset=utf-8",
            dataType: "html",
            data: JSON.stringify({
                BUCode: BUCode,
                pageNo: pageNo,
                recordPerPage: recordPerPage,
            }),
            success: function (result) {
                if (result == "true") {
                    redirectToLogin();
                }
                else {
                    $('#divDealDataList').empty();
                    $('#divDealDataList').html(result);
                    SetPaging();
                   // GetMURStatus();
                    initializeTooltip();
                    initializeExpander();
                }
            },
            error: function (result) {
                alert('Error: ' + result.responseText);
            }
        });
    }

    function DownloadReport(MURCode) {
        debugger;
            $.ajax({
                type: "POST",
                url: URL_DownloadReport,
                traditional: true,
                enctype: 'multipart/form-data',
                contentType: "application/json; charset=utf-8",
                data: JSON.stringify({
                    MURCode: MURCode
                }),
                success: function (result) {
                    debugger;
                    if (result.path == "")
                        showAlert('E',"File Not Found");
                    else {
                        debugger;
                        window.location.href = '@Url.Action("Download", "Reports")?MURCode=' + MURCode;
                    }
                },
                error: function (result) {
                    alert('Error: ' + result.responseText);
                }
            });
    }
    function RefreshReport(MURCode) {
        debugger;
        $.ajax({
            type: "POST",
            url: URL_RefreshReport,
            traditional: true,
            enctype: 'multipart/form-data',
            contentType: "application/json; charset=utf-8",
            dataType: "html",
            data: JSON.stringify({
                MURCode: MURCode
            }),
            success: function (result) {
                if (result == "true") {
                    redirectToLogin();
                }
                else {
                    if (result.Message != "") {
                        showAlert("S", "Data Refresh Successfully");
                    }
                    BindDealDataList();
                }
            },
            error: function (result) {
                alert('Error: ' + result.responseText);
            }
        });
    }
    function DeleteConfirmation(MURCode) {
        debugger
        $('#hdnDeleteId').val(MURCode);
        showAlert("I", "Are you sure, you want to delete this record?", 'OKCANCEL');
        return false;
    }
    </script>

