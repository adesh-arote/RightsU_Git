                        <td style="width:20%;">
                            <input type="text" id="txtAgreementNo" maxlength="20" style="width:221px" placeholder="@objMessageKey.AgreementNo" onkeyup="populateAgreementNo(this)" />
                            <input type="hidden" id="hdnAcqDealCode" value="0" />
                            <input type="hidden" id="hdnAgreementNo" value="0" />
                        </td>
                        <td style="width:10%;">
                            <label class="bold">
                                @objMessageKey.BusinessUnit
                            </label>
                        </td>
                        <td style="width:25%;">
                            @Html.DropDownList("BusinessUnit", ViewBag.BusinessUnitList as SelectList, new { @id = "ddlBusinessUnit", @class = "form_input chosen-select", @onchange = "BindPATitleList()" })
                        </td>
                        <td style="width:4%;">
                            <label class="bold">@objMessageKey.Title</label>
                        </td>
                        <td>
                            <input type="text" id="txtTitle" class="search" maxlength="100" style="width:285px" placeholder="@objMessageKey.Title" onkeyup="PopulateTitleNameForPA(event)" autocomplete="on" />
                            <input type="hidden" id="hdnTitleCode" value="0" />
                        </td>
                    </tr>
                    @if (ViewBag.isAdvAncillary == "Y")
                    {
                        <tr>
                            <td style="width:10%;">
                                <label class="bold">
                                    @objMessageKey.AncillaryType
                                </label>
                            </td>
                            <td style="width:20%;">
                                @Html.ListBox("AncillaryTypeList", ViewBag.AncillaryTypeList as MultiSelectList, new { @id = "ddlAncillaryType", @class = "multiple" })
                                @*@Html.DropDownList("AncillaryTypeList", ViewBag.AncillaryTypeList as MultiSelectList, "Please Select", new { @id = "ddlAncillaryType", @class = "form_input chosen-select" })*@
                            </td>
                            <td colspan="2">
                                <a title="@objMessageKey.SelectPlatforms" class="glyphicon glyphicon-flash" style="cursor: pointer;" onclick="BindPlatformPopup()"></a> <span id="selectedPlatofrms">@objMessageKey.SelectedPlatforms: 0</span>

                                <input type="hidden" id="hdnTabName" value="P" />
                                <input type="hidden" id="hdnTVCodes" name="hdnTVCodes" />
                                <input type="hidden" id="hdnPGCode" name="hdnPGCode" value="" />
                            </td>
                            <td colspan="2">
                                <input type="checkbox" id="IncludeExpired" />
                                Include Expired Titles
                            </td>
                            <td style="width:20%;text-align:right">
                                <input type="button" id="btnShow" class="btn btn-primary" value="Save" onclick="btnShow_OnClick()">
                                <input type="button" id="btnClearAll" class="btn btn-primary" value="@objMessageKey.Reset" onclick="ClearAll()">
                            </td>

                        </tr>
                    }
                    else
                    {
                        <tr>
                            <td colspan="4" style="width:20%;text-align:right">
                                <input type="button" id="btnShow" class="btn btn-primary" value="@objMessageKey.Show" onclick="btnShow_OnClick()">
                                <input type="button" id="btnClearAll" class="btn btn-primary" value="@objMessageKey.Reset" onclick="ClearAll()">
                            </td>
                        </tr>
                    }
                </table>
            </div>
            <div class="grid_area">
                <div style="border-bottom: 1px dotted #777; display:none;"></div>
                <div class="paging_area clearfix">
                    <span class="pull-left">Total Records: </span>
                    <span class="pull-left" id="lblRecordCount"></span>
                    <input type="hidden" id="hdnPageNo" name="hdnPageNo" value="1" />
                    <input type="hidden" id="hdnPagePerBatch" name="hdnPagePerBatch" value="5" />
                    <input type="hidden" id="hdnRecordCount" name="hdnRecordCount" value="0" />

                    <div id="Pagination" class="pagination">
                    </div>
                    <span class="pull-right">
                        Page Size
                        <input type="text" id="txtPageSize" name="txtPageSize" class="smallTextBox pagingSize" value="10" onchange="txtPageSize_OnChange()">
                    </span>
                </div>
                <div class="tab-content clearfix table-wrapper scale_table_container">
                    <div class="scale_table_block">
                        <div id="divDealDataList" class="deal clearfix"> </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
</div>

