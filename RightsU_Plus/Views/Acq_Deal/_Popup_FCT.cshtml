@model RightsU_Entities.Acq_Deal_Sport_Sales_Ancillary
@{
    MessageKey objMessageKey = new MessageKey();
    if (Session["objMessageKey"] != null)
    {
        objMessageKey = (MessageKey)Session["objMessageKey"];
    }
}
<style>
    .SumoSelect,#divlstSalesTitle{
        width:200px;
    }
</style>
<script type="text/javascript">
    $(document).ready(function () {
        loadAutoComplete();
        @if (Model.Acq_Deal_Sport_Sales_Ancillary_Code > 0 && ViewBag.Mode != "V" && ViewBag.Mode != "APRV" && ViewBag.Mode != "EWOA")
        {
            <text>
        var txtRemark = document.getElementById('Remarks');
        countChar(txtRemark);
        </text>
        }
        $(":radio[name=FRO_Given_Official_Sponsor]").change(function () {
            debugger;
            if ($(this).val() === 'N') {
                $('#Official_FRO_No_of_Days').val('');
                $('#Official_FRO_Validity').val('');
                $("#Official_FRO_No_of_Days").prop('disabled', true);
                $("#Official_FRO_Validity").prop('disabled', true);
            }
            else {
                $("#Official_FRO_No_of_Days").removeAttr('disabled');
                $("#Official_FRO_Validity").removeAttr('disabled');
            }
        });

        $(":radio[name=FRO_Given_Title_Sponsor]").change(function () {
            if ($(this).val() === 'N') {
                $('#Title_FRO_No_of_Days').val('');
                $('#Title_FRO_Validity').val('');
                $("#Title_FRO_No_of_Days").prop('disabled', true);
                $("#Title_FRO_Validity").prop('disabled', true);
            }
            else {
                $("#Title_FRO_No_of_Days").removeAttr('disabled');
                $("#Title_FRO_Validity").removeAttr('disabled');
            }
        });

        $(":radio[name=Last_Matching_Rights_Official_Sponsor]").change(function () {
            if ($(this).val() === 'N') {
                $('#Official_Last_Matching_Rights_Validity').val('');
                $("#Official_Last_Matching_Rights_Validity").prop('disabled', true);
            }
            else
                $("#Official_Last_Matching_Rights_Validity").removeAttr('disabled');
        });

        $(":radio[name=Last_Matching_Rights_Title_Sponsor]").change(function () {
            if ($(this).val() === 'N') {
                $('#Title_Last_Matching_Rights_Validity').val('');
                $("#Title_Last_Matching_Rights_Validity").prop('disabled', true);
            }
            else
                $("#Title_Last_Matching_Rights_Validity").removeAttr('disabled');
        });

        $('#Title_FRO_No_of_Days').numeric({ maxDigits: 3 });
        $('#Title_FRO_Validity').numeric({ maxDigits: 3 });
        $('#Official_FRO_No_of_Days').numeric({ maxDigits: 3 });
        $('#Official_FRO_Validity').numeric({ maxDigits: 3 });
        $('#Title_Last_Matching_Rights_Validity').numeric({ maxDigits: 3 });
        $('#Official_Last_Matching_Rights_Validity').numeric({ maxDigits: 3 });
    });

    function loadAutoComplete() {
        $.ajax({
            url: '@Url.Action("GetSponsorName", "Acq_Sport_Ancillary")',
            dataType: "json",
            type: "POST",
            contentType: "application/json; charset=utf-8",
            success: function (data) {
                if (data == "true") {
                    redirectToLogin();
                }
                $("input:text[id^='txtSponsor']").on("focus.autocomplete", null, function () {
                    $(this).autocomplete({
                        source: data,
                        minLength: 0,
                        delay: 0
                    });

                    $(this).autocomplete("search");
                });

                $("input:text[id^='txtOfficial']").on("focus.autocomplete", null, function () {
                    $(this).autocomplete({
                        source: data,
                        minLength: 0,
                        delay: 0
                    });

                    $(this).autocomplete("search");
                });
            },
            error: function (XMLHttpRequest, textStatus, errorThrown) {
                var err = eval("(" + XMLHttpRequest.responseText + ")");
                alert(err.Message)
                // console.log("Ajax Error!");  
            }
        });


    }

    function createtextbox(x) {
        $(x).remove();
        var sponsorId = parseInt($('#hdnSponsorId').val()) + 1;
        var txt = "<br id='brs" + sponsorId + "'/><input id='txtSponsor" + sponsorId + "' type='text' style='width:186px;'>";
        txt += "</input>";
        txt += "<input type='button' value='X' onclick='hideControl(this,txtSponsor" + sponsorId + ",brs" + sponsorId + ")'/>";
        txt += "<input id='btnAddTexSpo' type='button' value='+' onclick='createtextbox(this);' />";
        $("#sponsorlist").append(txt);
        $('#hdnSponsorId').val(sponsorId);
        if ($("#sponsorlist").find('input[type="text"]').length > 3) {
            $("#sponsorlist").css("overflow-y", "auto");
            $("#sponsorlist").css("height", "63px");
        }
        else {
            $("#sponsorlist").css("overflow-y", "hidden");
            $("#sponsorlist").css("height", "auto");
        }
        loadAutoComplete();
    }

    function Updatesponsorlist(id) {
        var Mode = '@ViewBag.Mode'
        if (Mode != 'V' && Mode != 'APRV' && Mode != 'EWOA') {
            var txtValue = $('#' + id.id).val();
            var listValue = $('#hdnSponserlist').val();
            var arr = listValue.split('~');
            if (listValue == '')
                $('#hdnSponserlist').val($('#' + id.id).val());
            else {
                if ($.inArray(txtValue, arr) == -1) {
                    $('#hdnSponserlist').val(listValue + '~' + txtValue);
                }
            }
        }
    }

    function createtextboxOfficial(x) {
        $(x).remove();
        var sponsorId = parseInt($('#hdnOfficialId').val()) + 1;
        var txt = "<br id='bro" + sponsorId + "'/><input id='txtOfficial" + sponsorId + "' type='text' style='width:186px;'>";
        txt += "</input>";
        txt += "<input type='button' value='X' onclick='hideControl(this,txtOfficial" + sponsorId + ",bro" + sponsorId + ")'/>"
        txt += "<input id='btnAddtxtOff' type='button' value='+' onclick='createtextboxOfficial(this);' />";
        $("#officiallist").append(txt);
        $('#hdnOfficialId').val(sponsorId);
        if ($("#officiallist").find('input[type="text"]').length > 3) {
            $("#officiallist").css("overflow-y", "auto");
            $("#officiallist").css("height", "63");
        }
        else {
            $("#officiallist").css("overflow-y", "hidden");
            $("#officiallist").css("height", "auto");
        }
        loadAutoComplete();
    }

    function UpdateOfficiallist(id) {
        var Mode = '@ViewBag.Mode'
        if (Mode != 'V' && Mode != 'APRV' && Mode != 'EWOA') {
            var txtValue = $('#' + id.id).val();
            var listValue = $('#hdnOfficiallist').val();
            var arr = listValue.split('~');
            if (listValue == '')
                $('#hdnOfficiallist').val(txtValue);
            else {
                if ($.inArray(txtValue, arr) == -1)
                    $('#hdnOfficiallist').val(listValue + '~' + txtValue);
            }
        }
    }

    function hideControl(button, textbox, br) {
        $(button).remove();
        $(br).remove();
        $(textbox).val("");
        $(textbox).remove();
        if ($("#sponsorlist").find('input[type="text"]').length > 3) {
            $("#sponsorlist").css("overflow-y", "auto");
            $("#sponsorlist").css("height", "63px");
        }
        else {
            $("#sponsorlist").css("overflow-y", "hidden");
            $("#sponsorlist").css("height", "auto");
        }
        if ($("#officiallist").find('input[type="text"]').length > 3) {
            $("#officiallist").css("overflow-y", "auto");
            $("#officiallist").css("height", "63");
        }
        else {
            $("#officiallist").css("overflow-y", "hidden");
            $("#officiallist").css("height", "auto");
        }
    }

    function getsponsorlist() {
        debugger;
        var Mode = '@ViewBag.Mode'
        if (Mode != 'V' && Mode != 'APRV') {
            var titles = $('#lstSalesTitle').val();
            if (titles === null) {
                $('#divlstSalesTitle').addClass('required');
                return false;
            }

            $('#hdnSponserlist').val('');
            $('#hdnOfficiallist').val('');
            $('#sponsorlist').find("input[type='text']").each(function (index) {
                //get value
                var val = $(this).val();
                if (val != '') {

                    var listValue = $('#hdnSponserlist').val();
                    var arr = listValue.split('~');
                    if (listValue == '')
                        $('#hdnSponserlist').val(val);
                    else {
                        if ($.inArray(val, arr) == -1) {
                            $('#hdnSponserlist').val(listValue + '~' + val);
                        }
                    }
                }
            });

            $('#officiallist').find("input[type='text']").each(function (index) {
                //get value
                var val = $(this).val();
                if (val != '') {

                    var listValue = $('#hdnOfficiallist').val();
                    var arr = listValue.split('~');
                    if (listValue == '')
                        $('#hdnOfficiallist').val(val);
                    else {
                        if ($.inArray(val, arr) == -1) {
                            $('#hdnOfficiallist').val(listValue + '~' + val);
                        }
                    }
                }
            });
            return true;
        }
        else
            return false;
    }

    function OnFCTSaveSuccess(message) {
        $('#popSportSalesAncillary').modal('hide');
        $('div.modal-backdrop').hide();
        var type = $(":radio[name=rdoSalesDisplayType]:checked").val();
        var arrTitles = ''
        if ($('#lblDisplaySalesTitle').val() != null)
            arrTitles = $('#lblDisplaySalesTitle').val().join(',');
        $.ajax({
            type: "POST",
            url: '@Url.Action("BindSales", "Acq_Sport_Ancillary")',
            traditional: true,
            enctype: 'multipart/form-data',
            contentType: "application/json; charset=utf-8",
            data: JSON.stringify({
                displayType: type,
                selectedTitle: arrTitles
            }),
            success: function (result) {
                $('#hdnAcqDealSportSalesAncillaryCode').val('');
                $('#divSales').html(result);
                initializeChosen();
                initializeExpander();
                $('#lblDisplaySalesTitle').val(arrTitles.split(',')).trigger("chosen:updated");
                $('input[name=rdoSalesDisplayType][value=' + type + ']').prop('checked', true);
            },
            error: function (result) {
                alert('Error: ' + result.responseText);
            }
        });
        CallFromDeleted = '';
        showAlert('S', message, "OK");
    }

</script>
<style>
    .SumoSelect,#divlstSalesTitle{
        width : 94% !important;
    }
     #divlstSalesTitle > div{
        width : 100% !important;
    }
     .form_wrapper{
         max-width : 900px !important;
     }
  
</style>

