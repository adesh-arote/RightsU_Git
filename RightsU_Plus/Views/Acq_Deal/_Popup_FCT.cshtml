                                                <td style="text-align: center;">
                                                    @if (Model.Title_FRO_No_of_Days != null)
                                                    {
                                                        @Model.Title_FRO_No_of_Days
                                                        <text> &nbsp;@objMessageKey.Days</text>
                                                    }
                                                </td>
                                                <td style="text-align: center;">
                                                    @if (Model.Title_FRO_Validity != null)
                                                    {
                                                        @Model.Title_FRO_Validity
                                                        <text> &nbsp;@objMessageKey.Days</text>
                                                    }
                                                </td>
                                            </tr>
                                            <tr>
                                                <td style="width: 110px;">@objMessageKey.OfficialSponsor
