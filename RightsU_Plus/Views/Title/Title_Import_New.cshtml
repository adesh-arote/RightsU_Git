                //$('.pagination').addClass('pagination-Head')
                // $('.right_nav').addClass('right_nav_Head')
            }
            addNumeric();
           // PopulateTitleGrid('0');
        });
        function addNumeric() {
            $(".pagingSize").numeric({
                allowMinus: false,
                allowThouSep: false,
                allowDecSep: false,
                max: 99,
                min: 1
            });
        }
        function UploadTitle() {
            debugger;
            if ("@Allow_Import_Movies_Shows" == "Y") {
                var SelectedTitleType = $("#TitleType:checked").val();
                if (SelectedTitleType == undefined) {
                    showAlert("E", "Please select type of title");
                    return false;
                }
            }
            else {
                SelectedTitleType = "M";
            }
            showLoading();
            var txtpageSize = $('#txtPageSize').val();
            $('#ddlFilterBy').val('A').trigger('chosen:updated');
            var FilterBy = $('#ddlFilterBy').val();
            var formdata = new FormData();
            var files = $("#fileUpload").get(0).files;
            if (files.length > 0) {
                formdata.append("InputFile", files[0]);
                formdata.append("FilterBy", FilterBy);
                formdata.append("txtpageSize", txtpageSize);
                formdata.append("SelectedTitleType", SelectedTitleType);
            }
            else {
                showAlert("a", "Please select File");
                hideLoading();
                return false;
            }

            $.ajax({

                type: "POST",
                url: '@Url.Action("UploadTitles", "DM_Title_Master_Import")',
                traditional: true,
                enctype: 'multipart/form-data',
                contentType: "application/json; charset=utf-8",
                async: true,
                contentType: false,
                processData: false,
                data: formdata,
                success: function (result) {
                    debugger;
                    $('#gvArea').html(result);
                    SetTitlePaging();
                    CheckPendingStatus();
                    hideLoading();
                },
                error: function (result) {
                    alert('Error: ' + result.responseText);
                }
            });

        }
        function PopulateTitleGrid(PageNo) {
            debugger
            var txtP = $('#txtPageSize').val();
            var FilterBy = $('#ddlFilterBy').val();
            if (txtP == null)
                txtP = 10;
            showLoading();
            $.ajax({

                type: "POST",
                url: '@Url.Action("PopulateTitleGrid", "DM_Title_Master_Import")',
                traditional: true,
                enctype: 'multipart/form-data',
                contentType: "application/json; charset=utf-8",
                data: JSON.stringify({
                    txtPageSize: txtP,
                    PgNo: PageNo,
                    FilterBy:FilterBy
                }),
                success: function (result) {
                    $('#gvArea').html(result);
                    SetTitlePaging();
                    CheckPendingStatus();
                    hideLoading();
                },
                error: function (result) {
                    alert('Error: ' + result.responseText);
                }
            });

        }
        function Page_OnChange() {
            $('.required').removeClass('required');
            $("[required='required']").removeAttr("required");
            var IsValid = ValidatePageSize();
            if (IsValid) {
                PopulateTitleGrid('0');

                SetTitlePaging();
            }
            else
                return false;
        }
        function ValidatePageSize() {
            var recordPerPage = $('#txtPageSize').val();
            if ($.trim(recordPerPage) != '') {
                var pageSize = parseInt(recordPerPage);
                if (pageSize > 0)
                    return true;
            }
            //$('#txtPageSize').addClass("required");
            return false;
        }
        //function Validate_Paging() {
        //    var pageSize = $('#txtPageSize').val();
