<script>

    var URL_BindPartialPages = '@Url.Action("BindPartialPages", "Vendor")';

    var URL_CheckRecordLock = '@Url.Action("CheckRecordLock", "Vendor")';
    var URL_Refresh_Lock = '@Url.Action("Refresh_Lock", "Global")';
    var URL_Release_Lock = '@Url.Action("Release_Lock", "Global")';
    var URL_Reload_Current_Page = '@Url.Action("Index", "Vendor")';
    var URL_AdditionalInfo = '@Url.Action("AdditionalInfo", "Vendor")';
    var URL_TnC = '@Url.Action("TnC", "Vendor")';
    var URL_SaveTnC = '@Url.Action("SaveTnC", "Vendor")';
    var URL_DeleteTnc = '@Url.Action("DeleteTnc", "Vendor")';
    var URL_ContentRule = '@Url.Action("ContentRule", "Vendor")';
    var URL_ContentRulePopup = '@Url.Action("ContentRulePopUp", "Vendor")';
    var URL_ContentRulePopUpGrid = '@Url.Action("ContentRulePopUpGrid", "Vendor")';
    var URL_SaveContentRuleCriteria = '@Url.Action("SaveContentPopupGrid", "Vendor")';
    var URL_DeleteContentRuleCriteria = '@Url.Action("DeleteContentPopupGrid", "Vendor")';
    var URL_SaveContentRule = '@Url.Action("SaveContentRule", "Vendor")';
    var URL_DeleteContentRule = '@Url.Action("DeleteContentRule", "Vendor")';
    var URL_OEM = '@Url.Action("OEM", "Vendor")';
    var URL_SaveOEM = '@Url.Action("SaveVendorOEM", "Vendor")';
    var URL_DeleteOEM = '@Url.Action("DeleteVendorOEM", "Vendor")';
    var URL_ChangeDataInputType = '@Url.Action("ChangeDataInputType", "Vendor")';
    var URL_BinddlColumnsValue = '@Url.Action("BinddlColumnsValue", "Vendor")';
    var URL_BindddlExtendedTalent = '@Url.Action("BindddlExtendedTalent", "Vendor")'
    var URL_SaveAdditionalInfo = '@Url.Action("SaveAdditionalInfo", "Vendor")';


    $(document).ready(function () {
        debugger;
       // $("#ancFileName").attr('href', '../Help/Masters/Layout.html?Party');
        BindPartialView("LIST",0);
    });


    //------------------------Record Locking-----------------

    function CheckRecordLock(VendorCode, commandName) {
        debugger;
        $.ajax({
            type: "POST",
            url: URL_CheckRecordLock,
            traditional: true,
            enctype: 'multipart/form-data',
            contentType: "application/json; charset=utf-8",
            data: JSON.stringify({
                VendorCode: VendorCode,
                commandName: commandName
            }),
            async: false,
            success: function (result) {
                if (result == "true") {
                    redirectToLogin();
                }
                else {
                    if (result.Is_Locked == "Y") {
                        debugger;
                        $("#hdnRecordislocked").val("Y")
                        BindPartialView("ADDEDIT", VendorCode)
                        $('#hdnRecodLockingCode_temp').val(result.Record_Locking_Code); // This syntax must be after AddEditCountry() this
                        Call_RefreshRecordReleaseTime(result.Record_Locking_Code, URL_Refresh_Lock);
                    }
                    else {
                        $("#hdnRecordislocked").val("N")
                        showAlert("E", result.Message);
                    }
                }
            },
            error: function (result) {
                alert('Error: ' + result.responseText);
                hideLoading();
            }
        });
    }

    function BindPartialView(key,VendorCode) {
        debugger
        if (key == "ADDEDIT") {
            Search = $('#searchCommon').val();
            $('#hdnSearch').val($('#searchCommon').val());
        }

        if (key == "EDIT") {
            Search = $('#searchCommon').val();
            $('#hdnSearch').val($('#searchCommon').val());
            CheckRecordLock(VendorCode, "EDIT")

            var isRCLock = $("#hdnRecordislocked").val()
            if (isRCLock == "N") {
                return false;
            }
        }

        $.ajax({
            type: "POST",
            url: URL_BindPartialPages,
            traditional: true,
            enctype: 'multipart/form-data',
            contentType: "application/json; charset=utf-8",
            dataType: "html",
            data: JSON.stringify({
                key: key,
                VendorCode: VendorCode
            }),
            success: function (result) {
                if (result == "true") {
                    redirectToLogin();
                }
                else {
                    $('#divVendor').empty();
                    $('#divVendor').html(result);
                    $('#ddlRoleList').SumoSelect();
                    $('#ddlTheatrical').SumoSelect();
                    $('#ddlCountry').SumoSelect();
                    $('#ddlRoleList')[0].sumo.reload();
                    $('#ddlTheatrical')[0].sumo.reload();
                    $('#ddlCountry')[0].sumo.reload();
                }
            },
            error: function (result) {
                alert('Error: ' + result.responseText);
            }
        });
    }

    //------------------------Additional Info-----------------

    function ShowAdditionalInfo(commandName, APVenderDetailCode) {
        debugger;
        $.ajax({
            type: "POST",
            url: URL_AdditionalInfo,
            traditional: true,
            enctype: 'multipart/form-data',
            contentType: "application/json; charset=utf-8",
            dataType: "html",
            data: JSON.stringify({
                CommandName: commandName,
                VendorDetailCode: APVenderDetailCode
            }),
            success: function (result) {
                debugger;
                $('#AdditionalInfoPopUp').modal();
                $('#popupHtml').empty();
                $('#popupHtml').html(result);
                if (commandName != "VIEW") {
                    commandName = commandName;
                }
                if ($('input[name="Vendor"]:checked').val() == "C") {
                    AdditionalInfoTab('CR', '', 0);
                }
                SwitchVendorClient();
                initializeChosen();
            }
        });
    }

    function SaveAdditionalInfo() {
        debugger;
        var returnVal = true;
        var VendorClient = $("input[name='Vendor']:checked").val();
        var VendorDetailCode = $("#hdnVendorDetailCode").val();
        var VendorCode = $("#hdnVendorCode").val();
        var ExtendedGroup = $("#ExtendedGroup").val();
        var BannerCode = "";
        var BannerValues = "";
        var PrefExclusionCode = "";
        var PrefExclusionValues = "";

        if (VendorClient == "" || VendorClient == undefined) {
            $("#Vendor").attr('required', true);
            returnVal = false;
            showAlert("E", "Please select if Party is a Vendor or a Client");
        }
        if (VendorClient == "V") {

            BannerCode = $("#Banner").val();
            if (BannerCode == "" || BannerCode == null) {
                showAlert("E", "Please select Banners");
                returnVal = false;
            }
            if (BannerCode != "" && BannerCode != null && BannerCode != undefined) {
                BannerCode = BannerCode.join(',');
                BannerValues = $("#Banner option:selected").map(function () {
                    return $(this).text();
                }).get().join();
            }
        }
        else if (VendorClient == "C") {

            PrefExclusionCode = $("#PrefExclusion").val();
            if (PrefExclusionCode == "" || PrefExclusionCode == null) {
                showAlert("E", "Please select Preference exclusions");
                returnVal = false;
            }
            if (PrefExclusionCode != "" && PrefExclusionCode != null && PrefExclusionCode != undefined) {
                PrefExclusionCode = PrefExclusionCode.join(',');
                PrefExclusionValues = $("#PrefExclusion option:selected").map(function () {
                    return $(this).text();
                }).get().join();
            }
        }

        if (returnVal) {
            $.ajax({
                type: "POST",
                url: URL_SaveAdditionalInfo,
                traditional: true,
                enctype: 'multipart/form-data',
                contentType: "application/json; charset=utf-8",
                data: JSON.stringify({
                    AL_Vendor_Detail_Code: VendorDetailCode,
                    PartyType: VendorClient,
                    Vendor_Code: VendorCode,
                    Banner_Codes: BannerCode,
                    Pref_Exclusion_Codes: PrefExclusionCode,
                    Extended_Group_Code_Booking: ExtendedGroup,
                    SelectedBannerValues: BannerValues,
                    SelectedPrefExclusionValues: PrefExclusionValues
                }),
                success: function (result) {
                    debugger;
                    //$('#AddInfoGrid').html(result);
                    ClosePopup();
                    showAlert("S", result.Message);
                }
            });
        }
    }

    function ClosePopup() {
        debugger;
        //Hide Add in Pop Up
        $('#hdnAction').val("");
        $('#popupHtml').empty();
        $('#AdditionalInfoPopUp').modal('hide');
    }

</script>
