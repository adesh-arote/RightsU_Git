                                    &nbsp;&nbsp;&nbsp;
                                    @Html.TextBox("Term_MM", Model.Term, "", new { @class = "smallTextBox numericTerm", @placeholder = "MM", @maxlength = 2 })
                                    <label style="margin-left:15px">End Date</label>
                                    @Html.TextBox("End_Date", string.Format("{0:" + GlobalParams.DateFormat + "}", Model.Right_End_Date), new { @class = "isDatepicker shortDate", @placeholder = "DD/MM/YYYY" })
                                </td>

                            </tr>
                        </tbody>
                    </table>
                </div>
                <div class="grid_area">
                    <div class="navigation_tabs">
                        <div class="tabbable">
                            <ul class="nav nav-tabs nav-tab pull-left">
                                <li class="active">
                                    @*<a data-toggle="tab" href="#tabrights" role="tab" id="titleName" style="display:none;"></a>*@

                                    @if (ViewBag.DealTypeCode == GlobalParams.Deal_Type_Content)
                                    {
                                        <a data-toggle="tab" href="#tabrights" class="tabText" role="tab">Program</a>
                                    }
                                    else if (ViewBag.DealTypeCode == GlobalParams.Deal_Type_Event)
                                    {
                                        <a data-toggle="tab" href="#tabrights" class="tabText" role="tab">Event</a>
                                    }
                                    else
                                    {
                                        <a data-toggle="tab" href="#tabrights" class="tabText" role="tab">Movie</a>
                                    }

                                </li>
                            </ul>
                            <div>
                                <a class="btn btn-primary AddDealTile" data-target="#popAddDealMovie" id="btnAddDealTile" data-toggle="modal">@objMessageKey.Add</a>
                            </div>
                            <div class="RunDef">
                                <input type="button" class="btn btn-primary" value="Run Def" onclick="GetEpF_EpT_ForRunDef();">
                            </div>
                            <div class="tab-content clearfix table-wrapper scale_table_container">
                                <div class="tab-pane active" id="tabrights">
                                    <div class="scale_table_block" id="tblDealFor">

                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <!--REMARKS-->
            <div class="grid_area">
                <div class="navigation_tabs">
                    <div class="tabbable">
                        <span style="float:right;" id="spnRemarkLength"></span>
                        <ul class="nav nav-tabs nav-tab pull-left">
                            <li class="active">
                                <a data-toggle="tab" href="#tabrights" role="tab">@objMessageKey.Remarks</a>
                            </li>
                        </ul>
                        <div class="tab-content clearfix table-wrapper scale_table_container">
                            <div class="tab-pane active" id="tabrights">
                                <div class="scale_table_block">
                                    @Html.TextAreaFor(m => m.Remarks, new { @class = "textarea restrictChar", @rows = "2", @cols = "20", @maxlength = "4000" })
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

            <!--BUTTONS-->
            <div class="bottom_action" style="padding-top:10px;">
                <input type="hidden" id="hdnIsSubmitted" name="IsSubmitted" value="N" />
                <ul class="form_link_nav clearfix">
                    <li>
                        <input type="submit" id="btnSave" class="btn btn-primary" value="@objMessageKey.Save" onclick="return ValidateSave('N')" />
                    </li>
                    <li>
                        <input type="submit" id="btnSaveSubmit" class="btn btn-primary" value="@objMessageKey.SaveandConfirm" onclick="return ValidateSave('Y')" />
                    </li>
                    <li>
                        <a class="btn btn-primary" onclick="CancelSaveDeal('')">@objMessageKey.Cancel</a>
                    </li>
                </ul>
            </div>
            <!--MODAL-->
            <div aria-labelledby="myModalLabel" class="form_wrapper modal fade big_popup" data-backdrop="static" data-keyboard="false" id="popup" role="dialog" tabindex="-1" aria-hidden="true" style="display: none; max-width:1094px">
                <div class="modal-dialog">
                    <div class="modal-content clearfix">
                        <div class="form_fields acq">
                            <div class="modal_block modal_header">
                                <a class="close" onclick="ClosePopup()">
                                    <span>X</span>
                                    <span class="sr-only">Close</span>
                                </a>
                                <h2>Run Definition</h2>
                            </div>
                            <div class="modal_block" style="padding: 10px 20px;">
                                <div id="Multiple_Title">
                                    <table>
                                        <tr>
                                            <td style="width:16%;"><b>@objMessageKey.Title</b></td>
                                            <td colspan="4">
                                                <div id="divRunDefTitle">
                                                    <select id='ddlRunDefTitle' class='' multiple='multiple'></select>
                                                </div>
                                            </td>
                                        </tr>
                                    </table>
                                    <table style="border-collapse: inherit;border-spacing: 4px;padding-left: 0px;">
                                        <tr>
                                            <td style="width:16%"><b>@objMessageKey.PrimeTime<b></td>
                                            <td style="width:10%"><span>Start Time:</span></td>
                                            <td style="width:15%"><input type="text" id="txtStartPrimeTime" class="PrimetimeRange" name="txtStartPrimeTime" /></td>
                                            <td style="width:10%"><span>End Time:</span></td>
                                            <td><input type="text" id="txtEndPrimeTime" class="PrimetimeRange" name="txtEndPrimeTime" /></td>
                                        </tr>
                                        <tr>
                                            <td style="width:16%"><b>Off Prime Time</b></td>
                                            <td><span>Start Time:</span></td>
                                            <td><input type="text" id="txtStartNPrimeTime" class="PrimetimeRange" name="txtStartNPrimeTime" /></td>
                                            <td><span>End Time:</span></td>
                                            <td><input type="text" id="txtEndNPrimeTime" class="PrimetimeRange" name="txtEndNPrimeTime" /></td>
                                            <td>
                                                <a class="btn btn-primary AddChannelrundef_class" style="float:right" disabled="true" id="AddChannelrundef" onclick="AddEditRunChannel('','ADD_RC')">Add Channels</a>
                                            </td>
                                        </tr>
                                    </table>
                                </div>
                                <div id="Single_Title" style="display:none">
                                    <input type="hidden" id="hdn_UniqueID" />
                                    <input type="hidden" id="hdn_Title_dummycode" value="" />
                                    <table>
                                        <tr>
                                            <td style="width:16%;"><b>@objMessageKey.Title</b></td>
                                            <td colspan="4">
                                                <span id="Title_Name"></span> ( <span id="Title_Start_Date"></span> - <span id="Title_End_Date"></span> )
                                            </td>
                                        </tr>
                                    </table>
                                    <table style="border-collapse: inherit;border-spacing: 4px;padding-left: 0px;">
                                        <tr>
                                            <td style="width:16%"><b>@objMessageKey.PrimeTime<b></td>
                                            <td style="width:10%"><span>Start Time:</span></td>
                                            <td style="width:15%"><input type="text" id="txtStartPrimeTime_Edit" class="PrimetimeRange" name="txtStartPrimeTime" /></td>
                                            <td style="width:10%"><span>End Time:</span></td>
                                            <td><input type="text" id="txtEndPrimeTime_Edit" class="PrimetimeRange" name="txtEndPrimeTime" /></td>
                                        </tr>
                                        <tr>
                                            <td style="width:16%"><b>Off Prime Time</b></td>
                                            <td><span>Start Time:</span></td>
                                            <td><input type="text" id="txtStartNPrimeTime_Edit" class="NonPrimetimeRange" name="txtStartNPrimeTime" /></td>
                                            <td><span>End Time:</span></td>
                                            <td><input type="text" id="txtEndNPrimeTime_Edit" class="NonPrimetimeRange" name="txtEndNPrimeTime" /></td>
                                            <td colspan="5">
                                                <a class="btn btn-primary AddChannelrundef_class" disabled="true" id="AddChannelrundef" onclick="AddEditRunChannel('','ADD_RC')">Add Channels</a>
                                            </td>
                                        </tr>
                                    </table>
                                </div>
                                <input type="hidden" id="hdnAction" value="" />
                                <div id="pupupHtml"></div>
                            </div>
                            <div class="modal_block_full">
                                <div class="form_links" style="padding: 10px;">
                                    <a class="btn btn-primary" onclick="Save_Update_Run_Defination();">@objMessageKey.Save</a>
                                    <a class="btn btn-primary" onclick="Cancel_Run_Defination();">@objMessageKey.Cancel</a>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>


            <div aria-labelledby="myModalLabel" class="form_wrapper modal fade in small_popup" data-backdrop="static" data-keyboard="false" id="popAddDealMovie" role="dialog" tabindex="-1" aria-hidden="false">
                <div class="modal-dialog" style="width: 100%;">
                    <div class="modal-content clearfix">
                        <form class="form" id="movie_popup">
                            <div class="form_wrapper" id="modalTitle">
                                <div class="form_fields acq">
                                    <div class="modal_block modal_header">
                                        <a class="close" data-dismiss="modal" href="#">
                                            <span>×</span>
                                            <span class="sr-only">Close</span>
                                        </a>
                                        <h2>Add Deal Title</h2>
                                    </div>
                                    <div class="modal_block">

                                        <input type="text" id="txtSearchTitle" class="" style="width:76%;height:30px;" placeholder="Search by Title,StarCast,Genres,Year of Production">
                                        <a class="btn btn-primary" id="btnSearchTitle" onclick="searchTitle()">@objMessageKey.Search</a>
                                        <ul class="form_fields_wrapper add_title_popup" style="padding-top:15px;">
                                            <li>
                                                <select id="ddlTitle" multiple="multiple" name="Title"></select>
                                            </li>
                                        </ul>

                                    </div>
                                    <div class="modal_block_full">
                                        <div class="form_links">
                                            <ul class="modal_block clearfix">
                                                <li>
                                                    <a class="btn btn-primary" id="btnSaveTitle" onclick="btnSaveTitle_OnClick()">@objMessageKey.Save</a>
                                                </li>
                                                <li>
                                                    <a data-dismiss="modal" class="btn btn-primary" href="#" onclick="CancelTitle()">@objMessageKey.Cancel</a>
                                                </li>
                                            </ul>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>
}
