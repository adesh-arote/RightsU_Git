<script>
    debugger;
    var URL_Cancel = '@Url.Action("Cancel", "Pro_List")';
    var URl_GetEpF_EpT_ForRunDef = '@Url.Action("GetEpF_EpT_ForRunDef","Pro_List")';
    var URL_GetProDealTitleNotIn = '@Url.Action("GetProDealTitlesNotIn", "Pro_List")';
    var recordLockingCode = 0;
    recordLockingCode = @ViewBag.RecordLockingCode;
    $('#hdnRecordLockingCode').val(recordLockingCode);
    // Start Record Locking Code
    var RecordLockInterval;
    function ReleaseRecordLock(code, url) {
        if (code != undefined && url != undefined)
            RefreshRecordReleaseTime(code, url)

        RecordLockInterval = window.clearInterval(RecordLockInterval);
    }
    function RefreshRecordReleaseTime(code, url) {
        $.ajax({
            type: "POST",
            url: url,
            traditional: true,
            enctype: 'multipart/form-data',
            contentType: "application/json; charset=utf-8",
            data: JSON.stringify({
                RLCode: code
            }),
            success: function (result) { },
            error: function (result) { }
        });
    }
    function Call_RefreshRecordReleaseTime(code, url) {
        RefreshRecordReleaseTime(code, url);
        RecordLockInterval = setInterval("RefreshRecordReleaseTime(" + code + ",'" + url + "')", 15000);
    }
    // End Record Locking Code
    function GetProDealTitles(DealForCode, searchString) {
        debugger;
        var TitleCode = '@ViewBag.TitleCode';
        var searchString = $.trim($("#txtSearchTitle").val());
        var TitleCodes_A = $("#hdn_Title_Codes").val();
        var dealTypeCode = GetDealTypeCode();
        if (dealTypeCode == "1") {
            if (TitleCode != "" || TitleCodes_A != "") {
                if (TitleCodes_A != "") {
                    TitleCode = TitleCodes_A;
                }
                $.ajax({
                    type: "POST",
                    url: URL_GetProDealTitleNotIn,
                    traditional: true,
                    enctype: 'multipart/form-data',
                    contentType: "application/json; charset=utf-8",
                    async: false,
                    data: JSON.stringify({
                        DealForCode: dealTypeCode,
                        searchString: searchString,
                        removeTitleCode: TitleCode,
                        action: "A"
                    }),
                    success: function (result) {
                        debugger;
                        if (result == "true") {
                            redirectToLogin();
                        }
                        else {
                            $("#ddlTitle").html("");
                            $.each(result, function (i, Form_ID) {
                                $("#ddlTitle").append($('<option></option>').val(result[i].Title_Code).html(result[i].Title_Name));
                            });
                            $('#ddlTitle')[0].sumo.reload();
                        }
                    },
                    error: function (result) { }
                });
            } else {
                $.ajax({
                    type: "POST",
                    url: URL_Pro_Deal_Title,
                    traditional: true,
                    enctype: 'multipart/form-data',
                    contentType: "application/json; charset=utf-8",
                    async: false,
                    data: JSON.stringify({
                        DealForCode: DealForCode,
                        searchString: searchString
                    }),
                    success: function (result) {
                        $("#ddlTitle").html("");
                        $.each(result, function (i, Form_ID) {
                            $("#ddlTitle").append($('<option></option>').val(result[i].Title_Code).html(result[i].Title_Name));
                        });
                        $('#ddlTitle')[0].sumo.reload();
                    },
                    error: function (result) { }
                });
            }
        } else {
            $.ajax({
                type: "POST",
                url: URL_Pro_Deal_Title,
                traditional: true,
                enctype: 'multipart/form-data',
                contentType: "application/json; charset=utf-8",
                async: false,
                data: JSON.stringify({
                    DealForCode: DealForCode,
                    searchString: searchString
                }),
                success: function (result) {
                    $("#ddlTitle").html("");
                    $.each(result, function (i, Form_ID) {
                        $("#ddlTitle").append($('<option></option>').val(result[i].Title_Code).html(result[i].Title_Name));
                    });
                    $('#ddlTitle')[0].sumo.reload();
                },
                error: function (result) { }
            });
        }
    }
    function searchTitle() {
        var _searchString = $("#txtSearchTitle").val();
        GetProDealTitles(DealforCode, _searchString);
    }

    function CancelTitle() {
        $("#txtSearchTitle").val("");
    }

    function AutoPopulateTermEdit() {
        var _txtStartDate = $('.Right_Start_Date');
        var _txtEndDate = $('.Right_End_Date');
        var _rightSD = new Date(MakeDateFormate(_txtStartDate.val()));
        var _rightED = new Date(MakeDateFormate(_txtEndDate.val()));

        if (!isNaN(_rightSD) && !isNaN(_rightED)) {
            _rightED.setDate(_rightED.getDate() + 1);
            var term = CalculateTerm(_rightSD, _rightED);
            var arr = term.split('.');
            $('.Term_YY').val(arr[0]);
            $('.Term_MM').val(arr[1]);
        }
    }

    function Save_Success_ProDeal(result) {
        //hideLoading();
        if (result == "true") {
            redirectToLogin();
        }
        else {
            if (result.Status == "S") {
                BindPartialView("LIST", "0");
                showAlert("S", result.Message);
            } else {
                showAlert("E", result.Message);
            }
        }
    }

    function GetDealTypeCode() {
        var dealTypeCode = $("input[name='Deal_Type_Code']:radio:checked").val();
        if (dealTypeCode == undefined)
            dealTypeCode = 0;
        return dealTypeCode;
    }

    function GetEpF_EpT_ForRunDef() {

        if (!ValidateEpisodeOverlapping()) {
            return false;
        }

        var titleList = new Array();
        var tblProTitle = $("#tblProTitle tr:not(:has(th))");

        var dealTypeCode = GetDealTypeCode();
        var dealCondition = GetDealTypeCondition(dealTypeCode);
        var i = 0;

        tblProTitle.each(function () {
            var _titleCode = 0, _dummyGuid = "", _episode_from = 1, _episode_to = 1;
            _titleCode = parseInt($("#hdnTitleCode_" + i).val());
            _dummyGuid = $("#hdnDummyGuid_" + i).val();
            if (dealCondition == Deal_Program) {
                _episode_from = parseInt($('#txtEpisode_From_' + i).val());
                _episode_to = parseInt($('#txtEpisode_To_' + i).val());
            }

            titleList.push({
                Title_Code: _titleCode, _Dummy_Guid: _dummyGuid, Episode_From: _episode_from,
                Episode_To: _episode_to
            });
            i++;
        });

        $.ajax({
            type: "POST",
            url: URl_GetEpF_EpT_ForRunDef,
            traditional: true,
            enctype: 'multipart/form-data',
            contentType: "application/json; charset=utf-8",
            data: JSON.stringify({
                titleList: titleList
            }),
            async: false,
            success: function (result) {
                if (result == "true") {
                    redirectToLogin();
                } else {
                    BindRunDef('', 'ADD_RD', 'ADD');
                }
            },
            error: function (result) { }
        });
    }

    function btnSaveTitle_OnClick() {
        debugger;
        var arrTitleCodes = $("#ddlTitle").val();
        if (arrTitleCodes == null) {
            $(".sumo_Title").addClass("required");
            return false;
        }
        if (arrTitleCodes != null) {
            titleCodes = arrTitleCodes.join(',');
            var Start_Date = $('#Start_Date').val();
            var End_Date = $("#End_Date").val();
            var Term_YY = $("#Term_YY").val();
            var Term_MM = $("#Term_MM").val();

            $.ajax({
                type: "POST",
                url: URL_SaveTitle,
                traditional: true,
                enctype: 'multipart/form-data',
                contentType: "application/json; charset=utf-8",
                async: false,
                data: JSON.stringify({
                    titleCodes: titleCodes,
                    dealTypeCode: DealforCode,
                    startdate: Start_Date,
                    enddate: End_Date,
                    month: Term_MM,
                    year: Term_YY
                }),
                success: function (result) {
                    debugger;
                    if (result == "true") {
                        redirectToLogin();
                    }
                    else {
                        if (arrTitleCodes != null) {
                            $("#popAddDealMovie").modal("hide");
                        }
                        TitleCode = result.TitleCode;
                        if (GetDealTypeCode() != "1") {
                            if (TitleCode != null) {
                                $("#ddlTitle").val(TitleCode.split(",")).attr("selected", "true")[0].sumo.reload();
                            }
                        }
                        BindListTitle(0, "");
                        if ($("#tblProTitle tr:not(:has(th))").length > 0) {
                            $('input[name="Deal_Type_Code"]').attr('disabled', true);
                        }
                        $(".tabText").text($("input:radio:disabled:checked").parent("label").text());
                        $("#txtSearchTitle").val("");
                        $("#hdn_Title_Codes").val(TitleCode);
                    }
                },
                error: function (result) { }
            });

        }
    }

    $("#btnAddDealTile").click(function (e) {
        debugger;
        var Start_Date = $('#Start_Date').val();
        var End_Date = $("#End_Date").val();
        var Term_YY = $("#Term_YY").val();
        var Term_MM = $("#Term_MM").val();
        var returnval = true;
        var arrTitleCodes = '@ViewBag.TitleCode';
        var TitleCodes_A = $("#hdn_Title_Codes").val();
        var dealTypeCode = GetDealTypeCode();
        var _searchString = $("#txtSearchTitle").val();
        GetProDealTitles(dealTypeCode, _searchString);

        if (arrTitleCodes != "" || TitleCodes_A != "") {
            if (dealTypeCode == "1") {
                if (TitleCodes_A != "") {
                    arrTitleCodes = TitleCodes_A;
                }
                var searchString = $.trim($("#txtSearchTitle").val());
                $.ajax({
                    type: "POST",
                    url: URL_GetProDealTitleNotIn,
                    traditional: true,
                    enctype: 'multipart/form-data',
                    contentType: "application/json; charset=utf-8",
                    async: false,
                    data: JSON.stringify({
                        DealForCode: dealTypeCode,
                        searchString: _searchString,
                        removeTitleCode: arrTitleCodes,
                        action: "A"
                    }),
                    success: function (result) {
                        debugger;
                        if (result == "true") {
                            redirectToLogin();
                        }
                        else {
                            $("#ddlTitle").html("");
                            $.each(result, function (i, Form_ID) {
                                $("#ddlTitle").append($('<option></option>').val(result[i].Title_Code).html(result[i].Title_Name));
                            });
                            $('#ddlTitle')[0].sumo.reload();
                            $("#txtSearchTitle").val("");
                        }
                    },
                    error: function (result) { }
                });
            }
        }


        if ($.trim(Start_Date) == "" || $.trim(End_Date) == "" || $.trim(Term_YY) == "" || $.trim(Term_MM) == "") {
            $("#Start_Date,#Term_YY,#Term_MM,#End_Date").attr('required', true);
            returnval = false;
        }
        return returnval;

        if (returnval == false) {
            e.stopPropagation();
        }
    });




    $(document).ready(function () {
        initializeChosen();
        $('#ddlTitle').SumoSelect();
        $('#ddlLicensors').SumoSelect();
        $('#ddlRunDefTitle').SumoSelect();
        var restrictCharLength = $(".restrictChar").val().length;
        $("#spnRemarkLength").html(restrictCharLength + "/4000");
        searchString = $("#txtSearchTitle").val();

        DealforCode = $("input:radio[name=Deal_Type_Code]:checked").val();
        var arrLicensorCodes = '@ViewBag.VendorCodes';
        if (arrLicensorCodes != null) {
            $("#ddlLicensors").val(arrLicensorCodes.split(",")).attr("selected", "true")[0].sumo.reload();
        }
        if ($("#Agreement_Date").val() != "") {
            $("#Agreement_Date").val($("#Agreement_Date").val().split(' ')[0]);
        }
        GetProDealTitles(DealforCode, searchString);
        AutoPopulateTerm();
        $(".isDatepicker").datepicker();
        $('#Start_Date').change(function () {
            SetMinDt();
            AutoPopulateTerm();
        });
        $('#End_Date').change(function () {
            SetMaxDt();
            AutoPopulateTerm();
        });
        $('#Term_YY, #Term_MM').change(function () {
            OnFocusLostTerm();
        });
        $("input[type=radio][name=Deal_For]").change(function () {
            DealforCode = $(this).val();
            GetProDealTitles($(this).val(), searchString);
        });
        $("input[type=radio][name=Deal_Type_Code]").change(function () {
            DealforCode = $(this).val();
            GetProDealTitles($(this).val(), searchString);
        });
        debugger;
        $('#hdnRecordLockingCode').val(recordLockingCode);
        if (recordLockingCode > 0)
            Call_RefreshRecordReleaseTime(recordLockingCode, URL_Refresh_Lock);

        AssignTimeJquery();
        AssignTimeJquery_Edit();
        $("#Agreement_Date").datepicker("option", "maxDate", new Date());
    });

    $(document).on('keyup', 'textarea', function () {
        var count = $(this).val().length;
        $("#spnRemarkLength").html(count + "/4000");
    });
</script>

