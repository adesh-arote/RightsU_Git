@model List<RightsU_Entities.USP_Title_Objection_List_Result>

<table class="table table-bordered table-hover" id="tblTitleObjection">
    <thead>
        <tr>
            <th style="width:25%;">Title</th>
            <th style="width:10%;">Agreement No</th>
            <th style="width:20%;">Licensor</th>
            <th style="width:25%;">Deal Description</th>
            <th style="width:10%;">Year Of Release</th>
            <th style="width:10%;">Action</th>
        </tr>
    </thead>
    <tbody>
        @if (Model.Count > 0)
        {
            foreach (var item in Model)
            {

                <tr>
                    <td>@item.Title</td>
                    <td>@item.Agreement_No</td>
                    <td>@item.Licensor</td>
                    <td>@item.Deal_Desc</td>
                    <td>@item.Year_Of_Production</td>
                    <td align="center">
                        @if (ViewBag.UserModuleRights.Contains("~" + UTOFrameWork.FrameworkClasses.GlobalParams.RightCodeForAdd + "~"))
                        {
                            <a title="Add Objection" class="glyphicon glyphicon-plus" style="cursor: pointer;" onclick="BindPartialView('EDIT','0')"></a>
                        }

                        @if (ViewBag.UserModuleRights.Contains("~" + UTOFrameWork.FrameworkClasses.GlobalParams.RightCodeForView + "~"))
                        {
                            <a title="View" class="glyphicon glyphicon-eye-open" style="cursor: pointer;" onclick="BindPartialView('VIEW','0')"></a>
                        }
                    </td>
                </tr>
            }
        }
        else
        {
            <tr>
                <td colspan="6">No Record Found</td>
            </tr>
        }
    </tbody>
</table>