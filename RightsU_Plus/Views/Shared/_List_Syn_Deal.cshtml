                                <script>
                                    $("#extend_@tempCount").append($('#View_@tempCount'));
                                </script>
                            }


                        }

                        @if (@objSynList.Show_Hide_Buttons.Contains("," + GlobalParams.RightCodeForEdit + ","))
                        {
                            Count = Count + 1;
