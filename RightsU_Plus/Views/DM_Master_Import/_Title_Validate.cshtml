@using UTOFrameWork.FrameworkClasses;
@model List<RightsU_Plus.Controllers.DM_Master_ImportController.Talent_Validation>
@if (Model.Count() > 0)
{
<div class="modal_block modal_header">
    <a class="close" onclick="ClosePopup()">
        <span>X</span>
        <span class="sr-only">Close</span>
    </a>
    <h2>Duplicate Talent Details </h2>
</div>
<br />
<div class="modal_block clearfix" style="margin-left: 20px;margin-right: 20px;">
    <div class="tab-content clearfix table-wrapper scale_table_container">
        <div class="scale_table_block" id="TalentValidation">
            <table class="table table-bordered table-hover">
                <thead>
                    <tr>
                        <th style="width:25%;">Talent</th>
                        <th style="width:25%;">Existing Role</th>
                    </tr>
                </thead>
                <tbody>
                    @if (Model.Count() > 0)
                    {
                        foreach (var item in Model)
                        {
                            <tr>
                                <td align="center">@item.Talent_Name</td>
                                <td align="center">@item.Role_Name</td>
                            </tr>
                        }
                    }

                </tbody>
            </table>
        </div>
    </div>
    <br />
</div>
}