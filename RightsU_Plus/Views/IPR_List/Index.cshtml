<script>
    var
    URL_CheckRecordLock = '@Url.Action("CheckRecordLock", "IPR_List")',
    URL_CheckRecordCurrentStatus = '@Url.Action("CheckRecordCurrentStatus", "IPR_List")',
    URL_Refresh_Lock = '@Url.Action("Refresh_Lock", "Global")',
    URL_Release_Lock = '@Url.Action("Release_Lock", "Global")';
    var panelVisible = false;
    var RightpanelHideCss = '-350px';
    var RightpanelShowCss = '0px';
    $(document).ready(function () {
        $('#ddlBusinessUnit').SumoSelect({ up: true });
        $('#ddlChannel').SumoSelect({ up: true });
        $('#ddlBusinessUnit')[0].sumo.reload();
        $('#ddlChannel')[0].sumo.reload();
        $('#hdnPageSizeForDosmestic_temp').val('0');
        $('#searchCommon').keypress(function (e) {
            debugger;
            if (e.keyCode == 13) {
                SearchCommon()
                return false;
            }
        });
        loadList('@ViewBag.tab', '0', 'N', 'N');
        if ('@ViewBag.isAdvanced' == 'Y')
            $('#divSearch').show();
        else
            $('#divSearch').hide();
        initializeChosen();
        initializeSubMenu2();
        AllsubmenuHide();
        var lastScrollTop = 0;

        $(window).scroll(function (event) {
            debugger;
            var st = $(this).scrollTop();
            if (st > lastScrollTop) {
                // downscroll code
                $("#pnlSearch").addClass("tinyADSearch")

            } else {
                // upscroll code
                $("#pnlSearch").removeClass("tinyADSearch")

            }
        });
       // $("#ancFileName").attr('href', '../Help/Masters/Layout.html?IPR_Application');
    })
    function Ask_Confirmation(IprCode, RowNum, tab) {
        $('#hdnCurrentTab').val(tab)
        var num = RowNum + '';
        $("#hdnRowNum").val(num);
        showAlert("I", 'Are you sure, you want to delete this record ?', "OKCANCEL");
    }
    function handleCancel() {
        $("#hdnRowNum").val('');
    }

    function handleOk() {
        debugger;
        showLoading();
        var RowNum = $("#hdnRowNum").val();
        if ($('#hdnCurrentTab').val() == 'A' || $('#hdnCurrentTab').val() == 'B')
            var objCode = $("#" + RowNum + "_hdnIPR_Opp_Code").val();
        else
            var objCode = $("#" + RowNum + "_hdnIPR_Rep_Code").val();

        $.ajax({
            type: "POST",
            url: URL_CheckRecordCurrentStatus,
            traditional: true,
            enctype: 'multipart/form-data',
            contentType: "application/json; charset=utf-8",
            async: true,
            data: JSON.stringify({
                IPR_Int_Opp_Code: parseInt(objCode)
            }),
            success: function (result) {
                hideLoading();
                if (result == "true") {
                    redirectToLogin();
                }
                else {
                    if (result.Is_Locked == "Y") {
                        debugger;
                        $('#hdnRecodLockingCode').val(result.Record_Locking_Code);
                        DeleteIPR(objCode, $('#hdnCurrentTab').val());
                        Call_RefreshRecordReleaseTime(result.Record_Locking_Code, URL_Refresh_Lock);
                        return true;
                    }
                    else {
                        showAlert("E", result.Message);
                        return false;
                    }
                }
            },
            error: function (result) {
                hideLoading();
                Command_Name_G = "";
                objSynListCode = 0;
                alert('Error in CheckRecordCurrentStatus() : ' + result.responseText);
            }
        });
        hideLoading();
    }

    function loadList(tab, pageNo, Istabchange, IsSetPage, recordPerPage) {
        debugger;
        if (tab == 'D') {
            recordPerPage = $('#txtPageSize').val()
            if (recordPerPage == undefined) {
                $('#hdnPageSizeForDosmestic_temp').val('10');
            }
        }
        if (tab == 'I') {
            recordPerPage = $('#txtPageSize_Int').val()
            if (recordPerPage == undefined) {
                $('#hdnPageSizeForInt_temp').val('10');
            }
        }
        if (tab == 'B') {
            recordPerPage = $('#txtPageSize_OppB').val()
            if (recordPerPage == undefined) {
                $('#hdnPageSizeForOppBy_temp').val('10');
            }
        }
        if (tab == 'A') {
            recordPerPage = $('#txtPageSize_OppA').val()
            if (recordPerPage == undefined) {
                $('#hdnPageSizeForOppAgainst_temp').val('10');
            }
        }
        var istabchange = Istabchange;
        var isSetPage = IsSetPage;
        if (Istabchange == 'Y')
            $('#searchCommon').val('');
        if (tab == 'D' || tab == 'I')
            $("#searchCommon").attr('placeholder', 'Application No., Type, Business Unit, Channel, Trademark, Trademark Attorney, Application Status, Applicant, Class, Country')
        else
            $("#searchCommon").attr('placeholder', 'Opposition No., Type, Business Unit, Channel, Trademark, Trademark Attorney, Class, Opposition Status')

        showLoading();
        tabChange(tab, istabchange);
        $.ajax({
            type: "POST",
            url: '@Url.Action("Bind_Grid_Domestic_List", "IPR_List")',
            traditional: true,
            enctype: 'multipart/form-data',
            contentType: "application/json; charset=utf-8",
            dataType: "html",
            data: JSON.stringify({
                Tab: tab,
                SearchCriteria: '',
                PageNo: pageNo,
                Istabchange: istabchange,
                IsSetPage: isSetPage,
                recordPerPage: recordPerPage
            }),
            success: function (result) {
                $('#gvArea').html(result);
                hideLoading();
            },
            error: function (result) {
                alert('Error: ' + result.responseText);
            }
        });
    }

    function AppChange() {
        $('#applicationNo').val($('#txtApplicationNo').val());
        $('#type').val($('#ddlType').val())
        $('#trademark').val($('#txtTradeMark').val())
        $('#type').val($('#txtTradeMarkAttorney').val())
        $('#appFromDate').val($('#txtAppFromDate').val())
        $('#appToDate').val($('#txtAppToDate').val())
        $('#applicationStatus').val($('#ddlApplicationStatus').val())
        $('#applicant').val($('#ddlApplicant').val())
        $('#renFromDate').val($('#txtRenFromDate').val())
        $('#renToDate').val($('#txtRenToDate').val())
        $('#type').val($('#txtClass').val())
        $('#useFromDate').val($('#txtUseFromDate').val())
        $('#useToDate').val($('#txtUseToDate').val())
        $('#oppositionNo').val($('#txtOppositionNo').val())
        $('#oppStatus').val($('#ddlOppStatus').val())
        $('#country').val($('#ddlCountry').val())
        $('#tab').val($('#hdnCurrentTab').val())
        $('#registrationNo').val($('#txtRegistrationNo').val())
    }
    function SearchCommon() {
        if ($('#searchCommon').val() != "") {
            clearField();
            $('#divSearch').hide();
        }
        SearchList('C', '0');
    }
    function SearchList(mode, pageNo) {
        debugger;
        var isAdvance = '';
        if (mode == 'C') {
            clearField();
        }
        showLoading();
        var ApplicationNo = $('#txtApplicationNo').val().trim()
        var Type = $('#ddlType').val().trim()
        var Trademark = $('#txtTradeMark').val().trim()
        var TradeMarkAttorney = $('#txtTradeMarkAttorney').val().trim()
        var AppFromDate = $('#txtAppFromDate').val().trim()
        var AppToDate = $('#txtAppToDate').val().trim()
        var ApplicationStatus = $('#ddlApplicationStatus').val().trim()
        var Applicant = $('#ddlApplicant').val().trim()
        var RenFromDate = $('#txtRenFromDate').val().trim()
        var RenToDate = $('#txtRenToDate').val().trim()
        var Classt = $('#txtClass').val().trim()
        var UseFromDate = $('#txtUseFromDate').val().trim()
        var UseToDate = $('#txtUseToDate').val().trim()
        var OppositionNo = $('#txtOppositionNo').val().trim()
        var OppStatus = $('#ddlOppStatus').val().trim()
        var Country = $('#ddlCountry').val().trim()

        var BusinessUnit = "";
        if ($('#ddlBusinessUnit').val() != null)
            BusinessUnit = $('#ddlBusinessUnit').val().toString();

        var Channel = "";
        if ($('#ddlChannel').val() != null)
            Channel = $('#ddlChannel').val().toString();

        var tabname = $('#hdnCurrentTab').val().trim()
        var srchCommon = $('#searchCommon').val().trim()
        var RegistrationNo = $('#txtRegistrationNo').val().trim()
        if (mode == 'S') {
            isAdvance = 'Y';
            $('#searchCommon').val('');
            if (ApplicationNo == "" && Type == "" && Trademark == "" && TradeMarkAttorney == "" && AppFromDate == "" && AppToDate == "" && ApplicationStatus == ""
            && Applicant == "" && RenFromDate == "" && RenToDate == "" && Classt == "" && UseFromDate == "" && UseToDate == "" && OppositionNo == ""
            && OppStatus == "" && Country == "" && RegistrationNo == "" && BusinessUnit == "" && Channel == "") {
                srchCommon = "";
                showAlert('e', 'Please select/enter search criteria');
                hideLoading();
                return false;
            }
        }

        $.ajax({
            type: "POST",
            url: '@Url.Action("SearchList", "IPR_List")',
            traditional: true,
            enctype: 'multipart/form-data',
            contentType: "application/json; charset=utf-8",
            dataType: "html",
            data: JSON.stringify({
                SearchCommon: srchCommon,
                Tab: tabname,
                applicationNo: ApplicationNo,
                type: Type,
                trademark: Trademark,
                tradeMarkAtto: TradeMarkAttorney,
                appFromDate: AppFromDate,
                appToDate: AppToDate,
                applicationStatus: ApplicationStatus,
                applicant: Applicant,
                renFromDate: RenFromDate,
                renToDate: RenToDate,
                classt: Classt,
                useFromDate: UseFromDate,
                useToDate: UseToDate,
                oppositionNo: OppositionNo,
                oppStatus: OppStatus,
                country: Country,
                businessUnit: BusinessUnit,
                channel: Channel,
                PageNo: pageNo,
                isAdvanced: isAdvance,
                registrationNo: RegistrationNo
            }),
            success: function (result) {
                $('#gvArea').html(result);
                hideLoading();
            },
            error: function (result) {
                alert('Error: ' + result.responseText);
            }
        });
    }

    function clearField() {
        $('#txtApplicationNo').val('');
        $('#ddlType').val('')
        $('#txtTradeMark').val('')
        $('#txtTradeMarkAttorney').val('')
        $('#txtAppFromDate').val('')
