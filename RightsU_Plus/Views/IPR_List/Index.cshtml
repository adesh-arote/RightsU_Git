        $('#ddlApplicant').val('')
        $('#txtRenFromDate').val('')
        $('#txtRenToDate').val('')
        $('#txtClass').val('')
        $('#txtUseFromDate').val('')
        $('#txtUseToDate').val('')
        $('#txtOppositionNo').val('')
        $('#ddlOppStatus').val('')
        $('#ddlCountry').val('')
        $('#ddlBusinessUnit').val('');
        $('#ddlChannel').val('');
        $('#txtRegistrationNo').val('')
    }
    function ShowAll() {
        $('#searchCommon').val('');
        clearField();
        loadList($('#hdnCurrentTab').val(), '0', 'Y', 'N');
    }

    function tabChange(tab, Istabchange) {
        if (Istabchange == 'Y')
            clearField();
        $('#hdnCurrentTab').val(tab);
        $('#Tab').val(tab);
        var h = $('#hdnCurrentTab').val();
        if (tab == "D" || tab == "I") {
            if (tab == "D") {
                $('#DomesticLi').addClass('active');
                $('#InternationalLi').removeClass();
                $('#OppositionByLi').removeClass();
                $('#OppositionAgainstLi').removeClass();
                $('#CountryLi').hide();
                $('#RegistrationLi').hide();
            }
            else {
                $('#InternationalLi').addClass('active');
                $('#DomesticLi').removeClass();
                $('#OppositionByLi').removeClass();
                $('#OppositionAgainstLi').removeClass();
                $('#CountryLi').show();
                $('#RegistrationLi').show();
            }
            $('#AppLi').show();
            $('#appdateLi').show();
            $('#rendateLi').show();
            $('#statusLi').show();
            $('#DateOfUseLi').show();
            $('#OppNoLi').hide();
            $('#OppStaLi').hide();
        }
        else {
            if (tab == "B") {
                $('#OppositionByLi').addClass('active');
                $('#InternationalLi').removeClass();
                $('#DomesticLi').removeClass();
                $('#OppositionAgainstLi').removeClass();
            }
            if (tab == "A") {
                $('#OppositionAgainstLi').addClass('active');
                $('#InternationalLi').removeClass();
                $('#DomesticLi').removeClass();
                $('#OppositionByLi').removeClass();
            }
            $('#OppNoLi').show();
            $('#OppStaLi').show();
            $('#AppLi').hide();
            $('#appdateLi').hide();
            $('#rendateLi').hide();
            $('#statusLi').hide();
            $('#DateOfUseLi').hide();
            $('#CountryLi').hide();
            $('#RegistrationLi').hide();
        }
    }
    function PageSize_OnChange() {
        $('.required').removeClass('required');
        $("[required='required']").removeAttr("required");
        var IsValid = ValidatePageSize();
        if (IsValid) {
            Bind_Grid(0, 'Y');
        }
        else
            return false;
    }
    function DeleteIPR(objCode, tab) {
        showLoading();
        $.ajax({
            type: "POST",
            url: '@Url.Action("DeleteIPR", "IPR_List")',
            traditional: true,
            enctype: 'multipart/form-data',
            async: false,
            contentType: "application/json; charset=utf-8",
            data: JSON.stringify({
                IPR_Code: objCode
            }),
            success: function (result) {
                debugger
                if (result == "true") {
                    redirectToLogin();
                }
                else {
                    if (result.Error != "")
                        showAlert(tab, result.Error);
                    else {
                        showAlert("S", result.Message);
                        loadList(tab, $("#hdnPageIndex").val(), 'N', 'N')
                    }
                    hideLoading();
                }
            },
            error: function (result) { }

        });
    }
    function initializeSubMenu2() {

        //$('.RightPanel').hide();
        //$('#sideNavi').show();
        $('.RightPanel li.has-sub>a').on('click', function () {
            $(this).removeAttr('href');
            var element = $(this).parent('li');
            if (element.hasClass('open')) {
                element.removeClass('open');
                element.find('li').removeClass('open');
                element.find('ul').slideUp(200);
            }
            else {
                element.addClass('open');
                element.children('ul').slideDown(200);
                element.siblings('li').children('ul').slideUp(200);
                element.siblings('li').removeClass('open');
                element.siblings('li').find('li').removeClass('open');
                element.siblings('li').find('ul').slideUp(200);
            }
        });
    }

    function AllsubmenuHide() {

        $('.RightPanel').css('right', RightpanelHideCss);
        $('.leftPanel').css('left', panelHideCss);
        $('#sideNavi').show();
        //$('.RightPanel').hide();
    }
    function togglePanelVisibility2(panelID) {
        document.getElementById('pnlSearch').removeAttribute('hidden')
        $('.leftPanel').css('left', panelHideCss);
        $(".RightPanel:not(#" + panelID + ")").css("right", RightpanelHideCss);
        var panel = document.getElementById(panelID);

        if (panel.style.right == RightpanelShowCss) {
            panel.style.right = RightpanelHideCss;
            panelVisible = false;
            $('#sideNavi').show();
            //$('#AdvanceSearchPanel').hide();
        }
        else {
            panel.style.right = RightpanelShowCss;
            panelVisible = true;
            $('#sideNavi').hide();
            //$('#AdvanceSearchPanel').show();
        }
    }

</script>
<script>

    function CheckRecordLock(Mode, IPR_Rep_Code, IPR_Opp_Code) {
        debugger;
        showLoading();
        var returnVal = false;
        $.ajax({
            type: "POST",
            url: URL_CheckRecordLock,
            traditional: true,
            enctype: 'multipart/form-data',
            contentType: "application/json; charset=utf-8",
            data: JSON.stringify({
                IPR_Rep_Code: IPR_Rep_Code,
                IPR_Opp_Code: IPR_Opp_Code
            }),
            async: true,
            success: function (result) {
                hideLoading();
                if (result == "true") {
                    redirectToLogin();
                }
                else {
                    if (result.Is_Locked == "Y") {
                        debugger;
                        if (Mode == "E") {
                            IPR_Opp_Code
                            if (IPR_Opp_Code == 0) {
                                var URL = '@Url.Action("ButtonEvent", "IPR_List", new
                                {
                                    MODE = "_MODE_",
                                    IPR_Rep_Code = "_IPR_Rep_Code_"
                                })';
                                URL = URL.replace("_MODE_", Mode);
                                URL = URL.replace("_IPR_Rep_Code_", IPR_Rep_Code);
                                URL = URL.replace(/amp;/g, "");
                                window.location.href = URL;
                            }
                            else if (IPR_Rep_Code == 0)
                            {
                                var URL = '@Url.Action("ButtonEvent", "IPR_List", new
                                {
                                    MODE = "_MODE_",
                                    IPR_Opp_Code = "_IPR_Opp_Code_"
                                })';
                                URL = URL.replace("_MODE_", Mode);
                                URL = URL.replace("_IPR_Opp_Code_", IPR_Opp_Code);
                                URL = URL.replace(/amp;/g, "");
                                window.location.href = URL;
                            }
                        }
                        else {
                            $('#hdnRecodLockingCode').val(result.Record_Locking_Code);
                            Call_RefreshRecordReleaseTime(result.Record_Locking_Code, URL_Refresh_Lock);
                        }
                        returnVal = true;
                    }
                    else {
                        showAlert("E", result.Message);
                        returnVal = false;
                    }
                }
            },
            error: function (result) {
                alert('Error in CheckRecordLock(): ' + result.responseText);
                hideLoading();
                returnVal = false;
            }
        });
        return returnVal;
    }
</script>
