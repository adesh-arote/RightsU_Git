<script type="text/javascript">
    $(document).ready(function () {
        initializeExpander();
        $('.numbertext').numeric({ allowMinus: false,
            allowThouSep: false,
            allowDecSep: false,
            maxPreDecimalPlaces: 3,
            maxDecimalPlaces: 0 });

        if(@Model.IPR_Rep_Code > 0)
