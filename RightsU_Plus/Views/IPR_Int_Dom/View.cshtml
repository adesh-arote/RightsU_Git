                                        <a type="button" id="btnCancelDeal" class="btn btn-primary" onclick="OnCancelClick();">Cancel</a>
                                    </li>
                                </ul>
                            </div>
                            <input type="hidden" id="hdnCommandName" />
                            <input type="hidden" id="hdnAttchRowIndex" />
                        </div>

                    </div>
                </div>
            </div>
        </div>
    </section>
